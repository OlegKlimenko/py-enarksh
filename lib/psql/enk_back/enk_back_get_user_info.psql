/**
 * Selects all resources of all nodes in a schedule revision.
 *
 * @param p_run_id The ID of the run.
 */
create procedure enk_back_get_user_info( in p_usr_login @aut_user.usr_login%type@ )
reads sql data
-- type: row1
begin
  select usr_login
  ,      usr_email
  from   AUT_USER
  where  usr_login = p_usr_login
  ;
end
